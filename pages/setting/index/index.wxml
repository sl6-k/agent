      <a class="weui_cell font-size-14" bindtap="modifyPassword">
        <view class="weui_cell_bd weui_cell_primary">
          <p>修改密码</p>
        </view>
        <view class="weui_cell_ft"></view>
      </a>
    </view>
    <view class="weui_cells weui_cells_access">
      <a class="weui_cell font-size-14" bindtap="setBank">
        <view class="weui_cell_bd weui_cell_primary">
