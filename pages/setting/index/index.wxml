        <view class="weui_cell_bd weui_cell_primary">
          <p>联系客服</p>
        </view>
        <view class="weui_cell_ft"></view>
      </a>
    </view>
    <view class="weui_cells weui_cells_access">
      <a class="weui_cell font-size-14" bindtap='about'>
        <view class="weui_cell_bd weui_cell_primary">
          <p>关于飞雳士</p>
        </view>
        <view class="weui_cell_ft"></view>
      </a>
    </view>
  </view>

  <view class='margin-t-15'>
    <view class="weui_cells weui_cells_access">
      <a class="weui_cell font-size-14" bindtap='bindUserSys'>
        <view class="weui_cell_bd weui_cell_primary">
